    "collaborators": [
        { "type": "pointer", "pointer": "/collaborators" },
        { "type": "join", "separator": ", " },
        { "type": "pointer", "pointer": "/login" }
    ],
    "date_created": [
        { "type": "pointer", "pointer": "/repo/created_at" }
    ],
    "date_updated": [
        { "type": "pointer", "pointer": "/repo/updated_at" }
    ],
    "date_pushed": [
        { "type": "pointer", "pointer": "/repo/pushed_at" }
