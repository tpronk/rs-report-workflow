    ],
    "installation_n": [
        { "type": "pointer", "pointer": "/somef/installation", "except": 0 },
        { "type": "count" }
    ],
    "executable_example_n": [
        { "type": "pointer", "pointer": "/somef/executable_example", "except": 0 },
        { "type": "count" }
    ],
    "acknowledgement_n": [
        { "type": "pointer", "pointer": "/somef/acknowledgement", "except": 0 },
